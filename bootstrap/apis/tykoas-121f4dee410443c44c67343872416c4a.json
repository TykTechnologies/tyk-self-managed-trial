{
  "components": {
    "securitySchemes": {
      "authToken": {
        "in": "header",
        "name": "Authorization",
        "type": "apiKey"
      }
    }
  },
  "info": {
    "title": "test",
    "version": "1.0.0"
  },
  "openapi": "3.0.3",
  "paths": {},
  "security": [
    {
      "authToken": []
    }
  ],
  "servers": [
    {
      "url": "http://localhost:8080/test/"
    }
  ],
  "x-tyk-api-gateway": {
    "info": {
      "name": "test",
      "state": {
        "active": true
      }
    },
    "middleware": {
      "global": {
        "contextVariables": {
          "enabled": true
        },
        "trafficLogs": {
          "enabled": true
        }
      }
    },
    "server": {
      "authentication": {
        "enabled": true,
        "securitySchemes": {
          "authToken": {
            "enabled": true
          }
        }
      },
      "listenPath": {
        "strip": true,
        "value": "/test/"
      }
    },
    "upstream": {
      "url": "http://httpbin.org/"
    }
  }
}